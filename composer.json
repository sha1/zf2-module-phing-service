{
  "name": "sha1/zf2-module-phing-service",
  "description": "Zend Framework 2 module for Phing, a PHP project build system or build tool based on Apache Ant.",
  "license": "MIT",
  "homepage": "https://github.com/bushbaby/zf2-module-phing-service",
  "authors": [
    {
      "name": "Bas Kamer",
      "email": "baskamer@gmail.com"
    }
  ],
  "keywords": [
    "zf2",
    "phing",
    "module"
  ],
  "require": {
    "php": ">=5.3.3",
    "phing/phing": "~2.4,>=2.4.13",
    "zendframework/zendframework": "~2.0",
    "symfony/process": "~2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~2.0",
    "phpunit/phpunit": "~4.1"
  },
  "autoload": {
    "psr-4": {
      "BsbPhingService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BsbPhingServiceTest\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
